	str_Extension = Split(str_DownloadedFilePath, ".").Last
	isFilePathValid = Not(dt_TempExtensions.AsEnumerable.
					  Any(Function(elem) elem("Extension").ToString = str_Extension))
End If]]></code></stage><stage stageid="0d85ea9b-464f-4298-8098-c307711e2065" name="isFilePathValid" type="Data"><subsheetid>90805e4b-a9f5-41cc-b60d-6fbbe5c165c1</subsheetid><display x="-390" y="135" w="90" h="30" /><datatype>flag</datatype><initialvalue /><private /><alwaysinit /></stage><stage stageid="bdbcca91-4588-4fbf-99dc-65f3a214d54a" name="date_CurrentDateTime" type="Data"><subsheetid>90805e4b-a9f5-41cc-b60d-6fbbe5c165c1</subsheetid><display x="-300" y="90" w="90" h="30" /><datatype>datetime</datatype><initialvalue /><private /><alwaysinit /></stage><stage stageid="cb11f2bc-1f9c-41f3-b77d-a7b6a6950a59" name="Get Current DateTime" type="Action"><subsheetid>90805e4b-a9f5-41cc-b60d-6fbbe5c165c1</subsheetid><display x="120" y="60" /><outputs><output type="datetime" name="Now" friendlyname="Now" narrative="Fecha Actual" stage="date_CurrentDateTime" /></outputs><onsuccess>67920765-d54a-4e96-abe2-2080d5eac0fc</onsuccess><resource object="Utility - Date and Time Manipulation" action="GetNow" /></stage><stage stageid="ac23cb1c-0b0a-47cb-98ff-b5883c879355" name="Arguments" type="Block"><subsheetid>bbc80380-48af-4e31-95ba-0400293dddb7</subsheetid><loginhibit /><display x="-270" y="-15" w="150" h="135" /><font family="Segoe UI" size="10" style="Regular" color="FFFF99" /></stage><stage stageid="a1ab9a7c-3f7c-4bf4-947a-76e539d865a2" name="Arguments" type="Block"><subsheetid>2e0f65f2-9995-439f-a327-d0142ff0c2f9</subsheetid><loginhibit /><display x="-225" y="-30" w="150" h="75" /><font family="Segoe UI" size="10" style="Regular" color="FFFF99" /></stage></process></object>
