/><display x="15" y="75" /><outputs><output type="flag" name="Success" stage="Exito" /><output type="collection" name="Values" stage="CSV Values" /><output type="number" name="UltimaPagina" stage="UltimaPagina" /></outputs></stage><stage stageid="4b0fc4c6-0550-48bc-85bc-bdc3f91ed2ec" name="CSV Location" type="Data"><subsheetid>3fda262a-7bbc-4960-ba4e-770a88814f73</subsheetid><display x="-225" y="-15" /><font family="Tahoma" size="10" style="Regular" color="000000" /><datatype>text</datatype><initialvalue xml:space="preserve">C:\TEMP\CLIENTES CONSULTA.csv</initialvalue><private /><alwaysinit /></stage><stage stageid="e2aa456a-ace2-4341-93b9-dcf05b59dc66" name="CSV Values" type="Collection"><subsheetid>3fda262a-7bbc-4960-ba4e-770a88814f73</subsheetid><display x="-225" y="75" /><font family="Tahoma" size="10" style="Regular" color="000000" /><datatype>collection</datatype><private /><alwaysinit /></stage><stage stageid="45c784f2-2fd3-4749-8fe8-1e3b5d488b04" name="First Line Is Header" type="Data"><subsheetid>3fda262a-7bbc-4960-ba4e-770a88814f73</subsheetid><display x="-225" y="30" /><font family="Tahoma" size="10" style="Regular" color="000000" /><datatype>flag</datatype><initialvalue>True</initialvalue><private /><alwaysinit /></stage><stage stageid="2d240b8c-8d61-4604-be57-285a9dd9e7cb" name="Block1" type="Block"><subsheetid>3fda262a-7bbc-4960-ba4e-770a88814f73</subsheetid><loginhibit /><display x="-285" y="-45" w="120" h="195" /><font family="Segoe UI" size="10" style="Regular" color="7FB2E5" /></stage><stage stageid="17bd5e83-12d7-44a0-ba63-aac51b126756" name="Get Collection Paginado" type="Code"><subsheetid>3fda262a-7bbc-4960-ba4e-770a88814f73</subsheetid><loginhibit /><display x="15" y="0" /><inputs><input type="text" name="path" expr="[CSV Location]" /><input type="flag" name="isHeader" expr="[First Line Is Header]" /><input type="text" name="caracter" expr="[Caracter]" /><input type="number" name="pagina" expr="[Pagina]" /><input type="number" name="numeroFilas" expr="[numeroFilas]" /></inputs><outputs><output type="text" name="mensaje" stage="Mensaje" /><output type="flag" name="exito" stage="Exito" /><output type="collection" name="dt" stage="CSV Values" /><output type="number" name="ultimapagina" stage="UltimaPagina" /></outputs><onsuccess>b305e3b0-d392-4add-a0ae-b5b27872226f</onsuccess><code><![CDATA[
Dim isinit As Boolean = true
Dim numCampo As Integer = 0
Dim cadena As String
Dim dato As String
Dim separador As Char = Convert.ToChar( caracter)
Dim lineas As String() = File.ReadAllLines(path, System.Text.Encoding.UTF8)
ultimapagina = Convert.ToInt32(Math.Ceiling(lineas.Length / numeroFilas))
Dim final as Integer = 0
Dim inicio as Integer = 0

dt = New DataTable()

If pagina > ultimapagina Then
	exito = false
	mensaje = "La página no puede ser mayor al numero de página total"
	return
End If

If lineas.Length < numeroFilas
	final = lineas.Length - 1
Else
	If pagina = 1 Then
		inicio = IIf(isHeader, 1, 0)
		final = numeroFilas	
	ElseIf pagina = ultimapagina Then
		inicio = IIf(isHeader, ((pagina - 1) * numeroFilas) + 1, ((pagina - 1) * numeroFilas))
		final = lineas.Length - 1	
	Else
		inicio = IIf(isHeader, ((pagina - 1) * numeroFilas) + 1, ((pagina - 1) * numeroFilas))
		final = (numeroFilas * pagina) 
	End If
End If

Try
	'El header siempre se debe incluir
	If isHeader Then
		cadena = lineas(0)
		Dim campos As String() = cadena.Split(separador)
		Dim row(0 To campos.Length - 1) As Object
		For Each dato in campos
			If isHeader Then
				dt.Columns.Add(dato, GetType(String))
			Else
				dt.Columns.Add("Campo_" &  numCampo.ToString(), GetType(String))
			End If
			numCampo += 1

		Next

	End If

	'Obtencion de datos
	For i as Integer = inicio to final step 1
		cadena = lineas(i)
		Dim campos As String() = cadena.Split(separador)
		Dim row(0 To campos.Length - 1) As Object
		numCampo = 0
		For Each dato in campos
			row(numCampo) = dato
			numCampo = numCampo + 1
		Next
		If campos.Length > 0 Then
			dt.Rows.Add(row)
		End IF
	Next
	exito = true

Catch err As Exception
