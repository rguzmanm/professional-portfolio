			For Each dato in campos
				If isHeader Then
					dt.Columns.Add(dato, GetType(String))
				Else
					dt.Columns.Add("Campo_" &  numCampo.ToString(), GetType(String))
				End If
				numCampo += 1

			Next
			numCampo = 0
			For Each dato in campos
				row(numCampo) = dato
				numCampo = numCampo + 1
			Next
			dt.Rows.Add(row)
			isinit = false

		Else
			numCampo = 0
			For Each dato in campos
				row(numCampo) = dato
				numCampo = numCampo + 1
			Next
			dt.Rows.Add(row)

		End if

	Next
	exito = true
	

Catch err As Exception
	mensaje = err.Message

End Try

